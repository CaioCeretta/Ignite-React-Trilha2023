#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules/loose-envify/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/node_modules:/home/jorge/Estudos/Rocketseat/node_modules:/home/jorge/Estudos/node_modules:/home/jorge/node_modules:/home/node_modules:/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules/loose-envify/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/node_modules:/home/jorge/Estudos/Rocketseat/node_modules:/home/jorge/Estudos/node_modules:/home/jorge/node_modules:/home/node_modules:/node_modules:/home/jorge/Estudos/Rocketseat/Ignite/React-Trilha-2023/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
